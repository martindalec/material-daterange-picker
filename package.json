{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "@martindalec/material-daterange-picker",
  "description": "Angular Material DateRange Picker",
  "version": "1.1.8",
  "keywords": [
    "angular",
    "angular2",
    "ngx",
    "material",
    "angular-material",
    "datepicker",
    "daterange-picker",
    "material-daterange-picker"
  ],
  "author": "Chris Martindale",
  "homepage": "https://github.com/martindalec/material-datepicker",
  "license": "MIT",
  "repository": "github:martindalec/material-datepicker",
  "bugs": {
    "url": "https://github.com/martindalec/material-datepicker/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --watch=false --no-progress --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "rimraf dist && ng-packagr -p ng-package.json",
    "gh-build": "ng build --prod --output-path docs --base-href",
    "dist": "npm run dist:pkg && node ./scripts/dist.js",
    "dist:pkg": "ng build daterangepicker -c production",
    "dist:scss": "scss-bundle -e ./daterangepicker/src/lib/theming.scss -d ./dist/_theming.scss",
    "commitmsg": "commitlint -e",
    "preupload": "npm run dist",
    "upload": "npm publish --access=public dist/",
    "release": "standard-version --message 'build: release v%s'"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular-devkit/build-ng-packagr": "~0.901.7",
    "@angular-devkit/core": "^9.1.7",
    "@angular/animations": "^9.1.7",
    "@angular/cdk": "^9.2.4",
    "@angular/cli": "~9.1.7",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/compiler-cli": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/forms": "^9.1.9",
    "@angular/language-service": "^9.1.9",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/router": "^9.1.9",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@types/moment-timezone": "^0.5.13",
    "core-js": "^3.6.5",
    "cpx": "^1.5.0",
    "fs-extra": "^9.0.1",
    "moment": ">= 2.24.0",
    "moment-timezone": ">= 0.5.28",
    "ng-packagr": "^9.1.2",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "scss-bundle": "^3.1.2",
    "standard-version": "^8.0.0",
    "tsickle": "^0.38.1",
    "tslib": "^1.11.1",
    "tslint": "~6.1.1",
    "typescript": "~3.8.3",
    "zone.js": "^0.10.3"
  }
}
